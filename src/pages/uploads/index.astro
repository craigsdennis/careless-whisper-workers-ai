---
export const prerender = false;

import Layout from "../../layouts/Layout.astro";

const {env} = Astro.locals.runtime;
const keys: string[] = await env.UPLOADER.listUploads();
---

<Layout>
    <h2>Previous Uploads</h2>
    <ul>
        {keys.map((key) => (
            <li><a href={"/uploads/" + key}>{key}</a></li>
        ))}
    </ul>
</Layout>
